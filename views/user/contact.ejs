
  <!--=== Contact Us Start ======-->
  <section class="contact-us white-bg" id="contact">
    <div class="container">
        <div class="container">
            <div class="row">
              <div class="col-md-6 col-lg-6 order-md-1 order-2 text-left">
                <h2>Get in Touch</h2>
                <h4 class="text-uppercase">Find your brand's voice</h4>
                <form name="contact-form"  action="/contact-form" method="POST" class="mt-50">
                  <div class="messages"></div>
                  <div class="form-group">
                    <label class="sr-only" for="name">Name</label>
                    <input type="text" name="name" class="form-control" id="name" required placeholder="Your Name" data-error="Your Name is Required">
                    <div class="help-block with-errors mt-20"></div>
                  </div>
                  <div class="form-group">
                    <label class="sr-only" for="email">Email</label>
                    <input type="email" name="email" class="form-control" id="email" placeholder="Your Email" required="required" data-error="Please Enter Valid Email" required>
                    <div class="help-block with-errors mt-20"></div>
                  </div>
                  <div class="form-group">
                    <label class="sr-only" for="phone">Your Phone</label>
                    <input type="phone" name="phone" class="form-control" id="phone" placeholder="Your Phone" required>
                    <div class="help-block with-errors mt-20"></div>
                  </div>
                  <div class="form-group">
                    <label class="sr-only" for="message">Message</label>
                    <textarea name="message" class="form-control" id="message" required  rows="7" placeholder="Your Message" required data-error="Please, Leave us a message"></textarea>
                    <div class="help-block with-errors mt-20"></div>
                  </div>
                  <button type="submit"  class="btn btn-color btn-circle">Send Message</button>
                </form>
              </div>
              <div class="col-md-6 col-lg-6 order-md-2 order-1">
                <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d778.1215340320774!2d76.39003402441475!3d10.619255798614208!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e0!3m2!1sen!2sin!4v1718281135255!5m2!1sen!2sin" width="100%" height="600" style="border: 1px solid #000; border-radius: 20px;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
              </div>
            </div>
          </div>
    </div>
  </section>
  <section class="p-0">
    <div class="container-fluid">
      <div class="row row-flex">
        <div class="col-md-4">
          <div class="col-inner spacer dark-bg">
            <div class="text-center white-color"> <i class="icofont icofont-google-map font-50px white-icon"></i>
              <h2>Office Address</h2>
              <p>Thrissur, Kerala <br>
                India</p>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="col-inner spacer gradient-bg">
            <div class="text-center white-color"> <i class="icofont icofont-email font-50px white-icon"></i>
              <h2>Email Us</h2>
              <p class="mb-0">adgroupap@gmail.com</p>
              <!-- <p>jobs@scoda.com</p> -->
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="col-inner spacer dark-bg">
            <div class="text-center white-color"> <i class="icofont icofont-iphone font-50px white-icon"></i>
              <h2>Call Us</h2>
              <p class="mb-0">+91 7902548312</p>
              <!-- <p></p> -->
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
  <!--=== Contact Us End ======-->

  <%if (clients) { %>
  <!--=== Clients Start ======-->
  <section class="pt-50 pb-50 white-bg">
    <div class="container">
      <div class="row">
        <div id="client-slider" class="owl-carousel">
          <% for (let i = 0; i < clients.length; i++) { %>
            <% let item = clients[i]; %>
          <div class="client-logo"> <img class="img-responsive" src="<%=item.image%>" alt="01"/> </div>
         <%}%>
        </div>
      </div>
    </div>
  </section>
  <!--=== Clients End ======-->
  <%}%>